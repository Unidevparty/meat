      <conf_key>dp3_fi_allowed_ext</conf_key>
      <conf_default>jpg,jpeg,png,gif</conf_default>
      <conf_extra/>
      <conf_evalphp/>
      <conf_protected>1</conf_protected>
      <conf_position>3</conf_position>
      <conf_start_group>Icons Settings</conf_start_group>
      <conf_add_cache>1</conf_add_cache>
      <conf_keywords/>
      <conf_title_keyword>dp3_fi</conf_title_keyword>
      <conf_is_title>0</conf_is_title>
    </setting>
