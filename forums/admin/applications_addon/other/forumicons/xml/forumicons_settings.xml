    <setting>
      <conf_title>Enable Mod?</conf_title>
      <conf_description/>
      <conf_group>79</conf_group>
      <conf_type>yes_no</conf_type>
      <conf_key>dp3_fi_enable</conf_key>
      <conf_default>0</conf_default>
      <conf_extra/>
      <conf_evalphp/>
      <conf_protected>1</conf_protected>
      <conf_position>1</conf_position>
      <conf_start_group>Main Settings</conf_start_group>
      <conf_add_cache>1</conf_add_cache>
      <conf_keywords/>
      <conf_title_keyword>dp3_fi</conf_title_keyword>
      <conf_is_title>0</conf_is_title>
    </setting>
    <setting>
      <conf_title>Display Mode</conf_title>
      <conf_description><![CDATA[Where you want to display the icons?<br /><br />
Next to the forum name<br />
Replace the indicator icon]]></conf_description>
      <conf_group>79</conf_group>
      <conf_type>dropdown</conf_type>
      <conf_key>dp3_fi_mode</conf_key>
      <conf_default>nextto</conf_default>
      <conf_extra>nextto=Next to the forum name&#13;
replace=Replace the indicator icon</conf_extra>
      <conf_evalphp/>
      <conf_protected>1</conf_protected>
      <conf_position>2</conf_position>
      <conf_start_group/>
      <conf_add_cache>1</conf_add_cache>
      <conf_keywords/>
      <conf_title_keyword>dp3_fi</conf_title_keyword>
      <conf_is_title>0</conf_is_title>
