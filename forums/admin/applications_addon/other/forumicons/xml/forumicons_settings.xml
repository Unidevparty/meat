      <conf_position>4</conf_position>
      <conf_start_group/>
      <conf_add_cache>1</conf_add_cache>
      <conf_keywords/>
      <conf_title_keyword>dp3_fi</conf_title_keyword>
      <conf_is_title>0</conf_is_title>
    </setting>
    <setting>
      <conf_title>Max. Dimensions</conf_title>
      <conf_description>Set maximum dimensions for the forum icons. (width x height).</conf_description>
