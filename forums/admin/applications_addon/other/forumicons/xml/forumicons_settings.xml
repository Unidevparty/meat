      <conf_title_desc>Settings for an application Forum Icons</conf_title_desc>
      <conf_title_tab>(DP3X) Mods</conf_title_tab>
      <conf_title_app>forumicons</conf_title_app>
      <conf_title_noshow>1</conf_title_noshow>
